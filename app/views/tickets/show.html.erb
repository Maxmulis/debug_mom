<h3>Description:</h3>
<%= @ticket.description  %>
<h3>Currently uploaded pictures:</h3>
<div>
  <% @ticket.photos.each do |photo| %>
    <%= cl_image_tag photo.key, height: 300, width: 400, crop: :fill %>
  <% end %>
</div>
Audio files:
<p>
  <video src="<%= url_for(@ticket.video) %>" height="200" width="300" loop controls></video>
</p>

<%= link_to "Yes, continue", summary_path, class: "btn btn-yellow btn-yellow-home" %>
<%= link_to "No, I want to change", edit_ticket_path(@ticket), class: "btn btn-black btn-black-home" %>
