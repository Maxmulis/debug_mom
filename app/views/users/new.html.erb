
<div class="users-container">
  <%= image_tag "contact.png", class: "contact-guy-img" %>


  <h1>What's your name and e-mail?</h1>
  <div class="form_users_container">

    <%= simple_form_for [ @ticket, @user ] do |f| %>
    <%= f.input :username, label: false, placeholder: 'Name', wrapper_html: { class: 'form_users' } %>
    <%= f.input :email, label: false, placeholder: 'Email', wrapper_html: { class: 'form_users' } %>
    <%= f.button :submit, "Submit", class: "btn-yellow btn-yellow-home" %>

    <% end %>
  </div>
</div>
 






